<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <withJansi>true</withJansi>
    <encoder>
        <pattern>%d{HH:mm:ss.SSS} [%thread] %highlight(%-5level) %cyan(%logger{36}).%M\(%line\) - %msg %n</pattern>
    </encoder>
</appender>

    <!--<logger name="blablub" level="DEBUG"/>-->

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
